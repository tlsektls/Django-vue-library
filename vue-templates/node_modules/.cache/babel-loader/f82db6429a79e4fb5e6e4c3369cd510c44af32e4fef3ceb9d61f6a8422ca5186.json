{"ast":null,"code":"import { Pagination, A11y, Navigation } from \"swiper\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\nexport default {\n  name: \"Swiper3\",\n  props: {\n    bookData: {}\n  },\n  data() {\n    return {\n      swiperOptions: {\n        slidesPerView: \"3\",\n        spaceBetween: \"30\",\n        slidesPerGroup: \"3\",\n        loop: \"true\",\n        pagination: {\n          el: '.swiper-pagination',\n          clickable: true\n        },\n        navigation: {\n          nextEl: '.swiper-button-next',\n          prevEl: '.swiper-button-prev'\n        }\n      }\n    };\n  },\n  components: {\n    Swiper,\n    SwiperSlide,\n    Navigation\n  },\n  created() {},\n  setup() {\n    return {\n      modules: [Pagination, Navigation, A11y]\n    };\n  }\n};","map":{"version":3,"mappings":"AA6CE,SAASA,UAAU,EAAEC,IAAI,EAAEC,UAAS,QAAS,QAAQ;AACrD,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;;AAGlD;AACA,OAAO,YAAY;AAEnB,OAAO,uBAAuB;AAC9B,OAAO,uBAAuB;AAG9B,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE;IACLC,QAAQ,EAAE,CAAC;EACb,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACRC,aAAa,EAAE;QACVC,aAAa,EAAE,GAAG;QAClBC,YAAY,EAAE,IAAI;QAClBC,cAAc,EAAE,GAAG;QACnBC,IAAI,EAAE,MAAM;QAChBC,UAAU,EAAE;UACXC,EAAE,EAAE,oBAAoB;UACnBC,SAAS,EAAE;QACjB,CAAC;QACDC,UAAU,EAAE;UACXC,MAAM,EAAE,qBAAqB;UAC7BC,MAAM,EAAE;QACT;MACD;IACD;EACA,CAAC;EACDC,UAAU,EAAE;IACVjB,MAAM;IACNC,WAAW;IACXF;EACF,CAAC;EACDmB,OAAM,GAAK,CAEX,CAAC;EACDC,KAAK,GAAG;IACN,OAAO;MACLC,OAAO,EAAE,CAACvB,UAAU,EAAEE,UAAU,EAAED,IAAI;IACxC,CAAC;EACH;AACF,CAAC","names":["Pagination","A11y","Navigation","Swiper","SwiperSlide","name","props","bookData","data","swiperOptions","slidesPerView","spaceBetween","slidesPerGroup","loop","pagination","el","clickable","navigation","nextEl","prevEl","components","created","setup","modules"],"sourceRoot":"","sources":["/Users/isg/Desktop/work/webstudy/Django-vue-library/vue-templates/src/components/inner/Swiper3.vue"],"sourcesContent":["<template>\n  <div class=\"swiper-container\">\n\n      <swiper\n    :modules=\"modules\"\n    :slides-per-view=\"3\"\n    :space-between=\"50\"\n    navigation\n    :pagination=\"{ clickable: true }\"\n    :scrollbar=\"{ draggable: true }\"\n    @swiper=\"onSwiper\"\n    @slideChange=\"onSlideChange\"\n  >\n    <swiper-slide>Slide 1</swiper-slide>\n    <swiper-slide>Slide 2</swiper-slide>\n    <swiper-slide>Slide 3</swiper-slide>\n    ...\n  </swiper>\n\n\n    <swiper\n      :slidesPerView=\"3\"\n      :spaceBetween=\"30\"\n      :loop=\"true\"\n      :pagination=\"{\n        clickable: true,\n      }\"\n      ref=\"mySwiperRef\"\n    >\n      <swiper-slide v-for=\"book in bookData\" :key=\"book\">\n        <router-link :to=\"{ name:'', params:{}}\">\n          <div class=\"img-wrap\"><img :src=\"'http://127.0.0.1:8000'+book.image\" alt=\"book-img\"/></div>\n          <p>{{ book.title }}</p>\n        </router-link>\n      </swiper-slide>\n    </swiper>\n\n    <div class=\"swiper-navigation\">\n      <button class=\"page-prev\" @click=\"this.$refs.mySwiperRef.$swiper.slidePrev()\"><font-awesome-icon icon=\"fa-solid fa-chevron-left\" /></button>\n      <button class=\"page-next\" @click=\"this.$refs.mySwiperRef.$swiper.slideNext()\"><font-awesome-icon icon=\"fa-solid fa-chevron-right\" /></button>\n    </div>\n  </div>\n</template>\n\n<script>\n  import { Pagination, A11y, Navigation } from \"swiper\";\n  import { Swiper, SwiperSlide } from \"swiper/vue\";\n\n\n// Import Swiper styles\nimport \"swiper/css\";\n\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\n\n\nexport default {\n  name: \"Swiper3\",\n  props: {\n    bookData: {}\n  },\n  data() {\n    return {\n\t\t\tswiperOptions: {\n        slidesPerView: \"3\",\n        spaceBetween: \"30\",\n        slidesPerGroup: \"3\",\n        loop: \"true\",\n\t\t\t\tpagination: {\n\t\t\t\t\tel: '.swiper-pagination',\n          clickable: true\n\t\t\t\t},\n\t\t\t\tnavigation: {\n\t\t\t\t\tnextEl: '.swiper-button-next',\n\t\t\t\t\tprevEl: '.swiper-button-prev',\n\t\t\t\t},                    \n\t\t\t},\n\t\t}\n  },\n  components: {\n    Swiper,\n    SwiperSlide,\n    Navigation\n  },\n  created () {\n\n  },\n  setup() {\n    return {\n      modules: [Pagination, Navigation, A11y],\n    };\n  },\n};\n</script>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}