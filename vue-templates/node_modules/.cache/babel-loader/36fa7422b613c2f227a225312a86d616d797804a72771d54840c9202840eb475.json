PIData\" :key=\"book.id\" class=\"col-md-4\">\n              <li>\n                <a href=\"{{ book.get_absolute_url }}\">{{ book.title }}</a> ({{book.author}})\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n\n\n    <Footer></Footer>\n\n  </div>\n</template>\n\n<script>\n  import { getAPI } from '../axios-api'\n  import Header from '../components/Header'\n  import Footer from '../components/Footer'\n  export default {\n    name: 'Books',\n    data () {\n      return {\n        NewsData: [],\n        APIData: [],\n      }\n    },\n    components: {\n      Header, \n      Footer\n    },\n    created () {\n      getAPI.get('/newsboard/',)\n        .then(response => {\n          console.log(response)\n          console.log('Django - book API has recieved data')\n          this.NewsData = response.data\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n@charset \"utf-8\";\n@import \"@/assets/scss/common.scss\";\n@import \"@/assets/scss/body.scss\";\n</style>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}