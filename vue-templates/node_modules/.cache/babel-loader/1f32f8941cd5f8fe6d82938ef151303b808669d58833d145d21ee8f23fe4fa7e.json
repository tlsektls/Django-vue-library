{"ast":null,"code":"import { getAPI } from '../axios-api';\nimport Navbar from '../components/Navbar';\nexport default {\n  name: 'Books',\n  data() {\n    return {\n      APIData: [],\n      DIData: []\n    };\n  },\n  components: {\n    Navbar\n  },\n  created() {\n    getAPI.get('/book/').then(response => {\n      console.log(response);\n      console.log('Post API has recieved data');\n      this.APIData = response.data;\n      //console.log(response.data)\n    }).catch(err => {\n      console.log(err);\n    });\n    getAPI.get('/bookInstance/').then(response => {\n      console.log(response);\n      console.log('Post API has recieved data');\n      this.DIData = response.data;\n      //console.log(response.data)\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n};","map":{"version":3,"mappings":"AAkCE,SAASA,MAAK,QAAS,cAAa;AACpC,OAAOC,MAAK,MAAO,sBAAqB;AACxC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,IAAG,GAAK;IACN,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE;IACV;EACJ,CAAC;EACDC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,OAAM,GAAK;IACPP,MAAM,CAACQ,GAAG,CAAC,QAAQ,CAAC,CACjBC,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ;MACpBC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxC,IAAI,CAACR,OAAM,GAAIM,QAAQ,CAACP,IAAG;MAC3B;IACF,CAAC,EACAU,KAAK,CAACC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,GAAG;IACjB,CAAC,CAAC;IAEJd,MAAM,CAACQ,GAAG,CAAC,gBAAgB,CAAC,CACzBC,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ;MACpBC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxC,IAAI,CAACP,MAAK,GAAIK,QAAQ,CAACP,IAAG;MAC1B;IACF,CAAC,EACAU,KAAK,CAACC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,GAAG;IACjB,CAAC;EACP;AACF","names":["getAPI","Navbar","name","data","APIData","DIData","components","created","get","then","response","console","log","catch","err"],"sourceRoot":"","sources":["/Users/isg/Desktop/work/webstudy/Django-vue-library/vue-templates/src/views/Books.vue"],"sourcesContent":["<template>\n  <div class=\"posts\">\n    <Navbar></Navbar> \n      <div class=\"album py-5 bg-light\">\n        <h3>BOOKS</h3>\n        <!--<div>{{APIData}}</div>-->\n        <div class=\"container\">\n            <div class=\"row\">\n              <h3>Books</h3>\n              <ul v-for=\"book in APIData\" :key=\"book.id\" class=\"col-md-4\">\n                <li>\n                  <a :href=\"book\">{{ book.title }}</a> ({{book.author}})\n                  <span>{{ book.language }}</span> / \n                  <span>{{ book.genre[0].name }}</span>\n\n                  <!--<p>{{ book.image }}</p>-->\n                  <!--<p>{{ book.image.url }}</p>-->\n                  <img :src=\"'http://127.0.0.1:8000' + book.image\" :alt=\"book.title + '-image'\" style=\"width:100px;\">\n                </li>\n              </ul>\n\n              <h3>BookInstance</h3>\n              <ul v-for=\"bookInstance in DIData\" :key=\"bookInstance.id\" class=\"col-md-4\">\n                <li>{{ bookInstance }}</li>\n              </ul>\n            </div>\n\n\n          </div>\n      </div>\n  </div>\n</template>\n\n<script>\n  import { getAPI } from '../axios-api'\n  import Navbar from '../components/Navbar'\n  export default {\n    name: 'Books',\n    data () {\n      return {\n          APIData: [],\n          DIData: [],\n        }\n    },\n    components: {\n      Navbar\n    },\n    created () {\n        getAPI.get('/book/',)\n          .then(response => {\n            console.log(response)\n            console.log('Post API has recieved data')\n            this.APIData = response.data\n            //console.log(response.data)\n          })\n          .catch(err => {\n            console.log(err)\n          });\n\n        getAPI.get('/bookInstance/',)\n          .then(response => {\n            console.log(response)\n            console.log('Post API has recieved data')\n            this.DIData = response.data\n            //console.log(response.data)\n          })\n          .catch(err => {\n            console.log(err)\n          })\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}