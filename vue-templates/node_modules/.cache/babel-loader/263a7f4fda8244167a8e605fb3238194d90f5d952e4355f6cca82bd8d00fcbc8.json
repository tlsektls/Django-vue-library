{"ast":null,"code":"import { getAPI } from '../axios-api';\nimport Header from '../components/Header';\nexport default {\n  name: 'Books',\n  data() {\n    return {\n      APIData: [],\n      DIData: [],\n      AuthorData: []\n    };\n  },\n  components: {\n    Navbar\n  },\n  created() {\n    getAPI.get('/book/').then(response => {\n      console.log(response);\n      console.log('Post API has recieved data');\n      this.APIData = response.data;\n    }).catch(err => {\n      console.log(err);\n    });\n    getAPI.get('/bookInstance/').then(response => {\n      console.log(response);\n      console.log('Post API has recieved data');\n      this.DIData = response.data;\n    }).catch(err => {\n      console.log(err);\n    });\n    getAPI.get('/author/').then(response => {\n      console.log(response);\n      console.log('Post API has recieved data');\n      this.AuthorData = response.data;\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n};","map":{"version":3,"mappings":"AA4CE,SAASA,MAAK,QAAS,cAAa;AACpC,OAAOC,MAAK,MAAO,sBAAqB;AACxC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,IAAG,GAAK;IACN,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,EAAE;MACVC,UAAU,EAAE;IACd;EACJ,CAAC;EACDC,UAAU,EAAE;IACVC;EACF,CAAC;EACDC,OAAM,GAAK;IACPT,MAAM,CAACU,GAAG,CAAC,QAAQ,CAAC,CACjBC,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ;MACpBC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxC,IAAI,CAACV,OAAM,GAAIQ,QAAQ,CAACT,IAAG;IAC7B,CAAC,EACAY,KAAK,CAACC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,GAAG;IACjB,CAAC,CAAC;IAEJhB,MAAM,CAACU,GAAG,CAAC,gBAAgB,CAAC,CACzBC,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ;MACpBC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxC,IAAI,CAACT,MAAK,GAAIO,QAAQ,CAACT,IAAG;IAC5B,CAAC,EACAY,KAAK,CAACC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,GAAG;IACjB,CAAC;IAEHhB,MAAM,CAACU,GAAG,CAAC,UAAU,CAAC,CACnBC,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ;MACpBC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxC,IAAI,CAACR,UAAS,GAAIM,QAAQ,CAACT,IAAG;IAChC,CAAC,EACAY,KAAK,CAACC,GAAE,IAAK;MACZH,OAAO,CAACC,GAA