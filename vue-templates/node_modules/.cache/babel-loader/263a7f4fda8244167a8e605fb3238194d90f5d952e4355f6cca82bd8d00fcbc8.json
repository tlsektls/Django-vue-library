      APIData: [],\n          DIData: [],\n          AuthorData: [],\n        }\n    },\n    components: {\n      Navbar\n    },\n    created () {\n        getAPI.get('/book/',)\n          .then(response => {\n            console.log(response)\n            console.log('Post API has recieved data')\n            this.APIData = response.data\n          })\n          .catch(err => {\n            console.log(err)\n          });\n\n        getAPI.get('/bookInstance/',)\n          .then(response => {\n            console.log(response)\n            console.log('Post API has recieved data')\n            this.DIData = response.data\n          })\n          .catch(err => {\n            console.log(err)\n          })\n\n        getAPI.get('/author/',)\n          .then(response => {\n            console.log(response)\n            console.log('Post API has recieved data')\n            this.AuthorData = response.data\n          })\n          .catch(err => {\n            console.log(err)\n          })\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}