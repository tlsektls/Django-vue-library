{ book.genre[0].name }}</span>\n\n                  <!--<p>{{ book.image }}</p>-->\n                  <!--<p>{{ book.image.url }}</p>-->\n                  <img :src=\"'http://127.0.0.1:8000' + book.image\" :alt=\"book.title + '-image'\" style=\"width:100px;\">\n                </li>\n              </ul>\n\n              <h3>BookInstance</h3>\n              <ul v-for=\"bookInstance in DIData\" :key=\"bookInstance.id\" class=\"col-md-4\">\n                <li>{{ bookInstance }}</li>\n              </ul>\n\n              <h3>BookInstance</h3>\n              <ul v-for=\"bookInstance in DIData\" :key=\"bookInstance.id\" class=\"col-md-4\">\n                <li>{{ bookInstance }}</li>\n              </ul>\n\n              <h3>author</h3>\n              <ul v-for=\"author in AuthorData\" :key=\"author.id\" class=\"col-md-4\">\n                <li>{{ author }}</li>\n              </ul>\n            </div>\n\n\n          </div>\n      </div>\n\n    <Footer></Footer>\n  </div>\n</template>\n\n<script>\n  import { getAPI } from '../axios-api'\n  import Header from '../components/Header'\n  import Footer from '../components/Footer'\n  export default {\n    name: 'Books',\n    data () {\n      return {\n          APIData: [],\n          DIData: [],\n          AuthorData: [],\n        }\n    },\n    components: {\n      Header,\n      Footer\n    },\n    created () {\n        getAPI.get('/newsboard/',)\n          .then(response => {\n            console.log(response)\n            console.log('newsboard API has recieved data')\n            this.APIData = response.data\n          })\n          .catch(err => {\n            console.log(err)\n          });\n\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}